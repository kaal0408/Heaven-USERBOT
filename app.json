{
    "name": "Heaven-Userbot",
    "description": "A First Telegram Userbot Project With Multi Client Based On PyroGram!",
    "logo": "https://telegra.ph/file/acd5abe21655b9576a279.jpg",
    "repository": "https://github.com/kaal0408/Heaven-Userbot",
    "env": {
        "STRING_SESSION1": {
            "description": "pyrogram string session here",
            "required": true
        }, 
        "API_ID": {
            "description": "telegram api id here",
            "required": true
        },
        "MONGO_DB": {
            "description": "Enter Your Mongo Database Url! This Is helpful for gban fban etc stuffs",
            "required": false
        },
        "LOG_GROUP": {
            "description": "Enter There group/ channel id where fbans etc stuffs logs ( Add @Pikachu_x_bot and type /id, if channel then forward a message to @Pikachu_x_bot.)",
            "required": false
        },
        "SUDO_USERS": {
            "description": "Who have Access Your Account But Rearly Plugins there",
            "required": true
        },
        "HEROKU_API": {
            "description": "Enter You Heroku Api there",
            "required": false
        },
        "HEROKU_APP_NAME": {
            "description": "Enter Your heroku App Name Here",
            "required": false
        },
        "API_HASH": {
            "description": "telegram api hash here",
            "required": true
        }
    },
    "addons": [
        {
            "plan": "heroku-postgresql"
        }],
    "buildpacks": [
            {
                "url": "heroku/python"
            },
            {
                "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
            }
        ]
}
